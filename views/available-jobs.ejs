                            <script>
                                


                                function sort(){

                                    var sort = document.getElementById('Sort').value
                                    if(sort == 'jobsRecent'){
                                        document.getElementById('jobsRecent').style.display = 'block'
                                        document.getElementById('jobsCreditsAsc').style.display = 'none'
                                        document.getElementById('jobsCreditsDesc').style.display = 'none'
                                        document.getElementById('jobsTitleAsc').style.display = 'none'
                                    }
                                    else if(sort == 'jobsCreditsAsc'){
                                        document.getElementById('jobsRecent').style.display = 'none'
                                        document.getElementById('jobsCreditsAsc').style.display = 'block'
                                        document.getElementById('jobsCreditsDesc').style.display = 'none'
                                        document.getElementById('jobsTitleAsc').style.display = 'none'
                                    }
                                    else if(sort == 'jobsCreditsDesc'){
                                        document.getElementById('jobsRecent').style.display = 'none'
                                        document.getElementById('jobsCreditsAsc').style.display = 'none'
                                        document.getElementById('jobsCreditsDesc').style.display = 'block'
                                        document.getElementById('jobsTitleAsc').style.display = 'none'
                                    }
                                    else if(sort == 'jobsTitleAsc'){
                                        document.getElementById('jobsRecent').style.display = 'none'
                                        document.getElementById('jobsCreditsAsc').style.display = 'none'
                                        document.getElementById('jobsCreditsDesc').style.display = 'none'
                                        document.getElementById('jobsTitleAsc').style.display = 'block'
                                    }

                                }
